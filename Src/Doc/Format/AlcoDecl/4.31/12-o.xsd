<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sch="http://purl.oclc.org/dsdl/schematron" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Файл">
		<xs:annotation>
			<xs:documentation>Декларация об объемах розничной продажи  пива и пивных напитков</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ФормаОтч">
					<xs:annotation>
						<xs:documentation>Описание формы отчетности</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element name="Первичная">
								<xs:annotation>
									<xs:documentation>Первичная форма отчетности</xs:documentation>
								</xs:annotation>
								<xs:complexType/>
							</xs:element>
							<xs:element name="Корректирующая">
								<xs:annotation>
									<xs:documentation>Первичная форма отчетности</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="НомерКорр" use="required">
										<xs:annotation>
											<xs:documentation>Номер корректировки</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:totalDigits value="3"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:choice>
						<xs:attribute name="НомФорм" use="required">
							<xs:annotation>
								<xs:documentation>Номер формы</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="2"/>
									<xs:enumeration value="12"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ПризПериодОтч" use="required">
							<xs:annotation>
								<xs:documentation>Отчетный период за квартал</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1"/>
									<xs:enumeration value="3"/>
									<xs:enumeration value="6"/>
									<xs:enumeration value="9"/>
									<xs:enumeration value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ГодПериодОтч" type="xs:gYear" use="required">
							<xs:annotation>
								<xs:documentation>Отчетный год</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Справочники">
					<xs:annotation>
						<xs:documentation>Справочная информация</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ПроизводителиИмпортеры" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Справочник производителей/импортеров</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:choice>
											<xs:element name="ЮЛ">
												<xs:annotation>
													<xs:documentation>Производитель/импортер - организация</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attribute name="П000000000005" type="ИННLiteТип" use="required">
														<xs:annotation>
															<xs:documentation>ИНН ЮЛ</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="П000000000006" type="КППLiteТип" use="required">
														<xs:annotation>
															<xs:documentation>КПП</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:complexType>
											</xs:element>
											<xs:element name="ФЛ">
												<xs:annotation>
													<xs:documentation>Производитель/импортер - физическое лицо</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attribute name="П000000000005" use="required">
														<xs:annotation>
															<xs:documentation>ИНН ФЛ/Учетный номер плательщика/Регистрационный номер налогоплательщика, либо бизнес-идентификационный номер, либо индивидуальный </xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="20"/>
																<xs:minLength value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:attribute>
												</xs:complexType>
											</xs:element>
										</xs:choice>
									</xs:sequence>
									<xs:attribute name="ИДПроизвИмп" type="xs:int" use="required">
										<xs:annotation>
											<xs:documentation>Уникальный идентификатор</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="П000000000004" use="required">
										<xs:annotation>
											<xs:documentation>Наименование производителя/импортёра</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="3"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Поставщики" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Справочник поставщиков</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:choice>
											<xs:element name="ЮЛ">
												<xs:annotation>
													<xs:documentation>Поставщик - организация</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attribute name="П000000000009" type="ИННLiteТип" use="required">
														<xs:annotation>
															<xs:documentation>ИНН ЮЛ</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="П000000000010" type="КППLiteТип" use="required">
														<xs:annotation>
															<xs:documentation>КПП</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:complexType>
											</xs:element>
											<xs:element name="ФЛ">
												<xs:annotation>
													<xs:documentation>Поставщик - физическое лицо</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:attribute name="П000000000009" use="required">
														<xs:annotation>
															<xs:documentation>ИНН ФЛ/Учетный номер плательщика/Регистрационный номер налогоплательщика, либо бизнес-идентификационный номер, либо индивидуальный </xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="20"/>
																<xs:minLength value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:attribute>
												</xs:complexType>
											</xs:element>
										</xs:choice>
									</xs:sequence>
									<xs:attribute name="ИдПостав" type="xs:int" use="required">
										<xs:annotation>
											<xs:documentation>Уникальный идентификатор</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="П000000000007" use="required">
										<xs:annotation>
											<xs:documentation>Наименование оргнанизации/ФИО для физ лица</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="3"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Документ">
					<xs:annotation>
						<xs:documentation>Общие сведения информационной части</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Организация">
								<xs:annotation>
									<xs:documentation>Сведения об организации</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Реквизиты">
											<xs:annotation>
												<xs:documentation>Сведения о реквизитах организации</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="АдрОрг" type="АдрТип">
														<xs:annotation>
															<xs:documentation>Местонахождение 
организации     
(обособленного  
подразделения)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:choice>
														<xs:element name="ЮЛ">
															<xs:annotation>
																<xs:documentation>Организация ЮЛ</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="required">
																	<xs:annotation>
																		<xs:documentation>ИНН ЮЛ</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
																<xs:attribute name="КППЮЛ" type="КППТип" use="required">
																	<xs:annotation>
																		<xs:documentation>КПП</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
															</xs:complexType>
														</xs:element>
														<xs:element name="ФЛ">
															<xs:annotation>
																<xs:documentation>Организация - физическое лицо</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:attribute name="ИННФЛ" type="ИННФЛТип" use="required">
																	<xs:annotation>
																		<xs:documentation>ИНН ФЛ</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
															</xs:complexType>
														</xs:element>
													</xs:choice>
												</xs:sequence>
												<xs:attribute name="Наим" use="required">
													<xs:annotation>
														<xs:documentation>Полное наименование организации/ физического лица</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ТелОрг" use="required">
													<xs:annotation>
														<xs:documentation>Контактный      
телефон</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0"/>
															<xs:maxLength value="20"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="EmailОтпр" use="required">
													<xs:annotation>
														<xs:documentation>E-mail</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="45"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="ОтветЛицо">
											<xs:annotation>
												<xs:documentation>Сведения об ответственных лицах организации</xs:documentation>
												<!--     Сведения о представителе присутствуют только если признак подписанта принимает значение 2          -->
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Руководитель">
														<xs:annotation>
															<xs:documentation>Сведения о руководителе</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="ОтветЛицо"/>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="Главбух" type="ОтветЛицо">
														<xs:annotation>
															<xs:documentation>Сведения о главном бухгалтере</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ОбъемОборота" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Cведения об объемах розничной продажи  пива и пивных напитков</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="АдрОрг" type="АдрТип">
											<xs:annotation>
												<xs:documentation>Местонахождение 
организации     
(обособленного  
подразделения)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Оборот" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Розничный оборот пива и пивных напитков</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="СведПроизвИмпорт" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Сведения по каждому производителю/импортеру</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Поставщик" minOccurs="0" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>Сведения по каждому поставщику</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Продукция" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:attribute name="П200000000013" type="ДатаТип" use="required">
																						<xs:annotation>
																							<xs:documentation>Дата закупки</xs:documentation>
																						</xs:annotation>
																					</xs:attribute>
																					<xs:attribute name="П200000000014" use="required">
																						<xs:annotation>
																							<xs:documentation>Номер товарно-транспортной наладной</xs:documentation>
																						</xs:annotation>
																						<xs:simpleType>
																							<xs:restriction base="xs:string">
																								<xs:maxLength value="50"/>
																							</xs:restriction>
																						</xs:simpleType>
																					</xs:attribute>
																					<xs:attribute name="П200000000015" use="required">
																						<xs:annotation>
																							<xs:documentation>Номер  таможенной декларации</xs:documentation>
																						</xs:annotation>
																						<xs:simpleType>
																							<xs:restriction base="xs:string">
																								<xs:maxLength value="50"/>
																							</xs:restriction>
																						</xs:simpleType>
																					</xs:attribute>
																					<xs:attribute name="П200000000016" use="required">
																						<xs:annotation>
																							<xs:documentation>Объем закупленной продукции</xs:documentation>
																						</xs:annotation>
																						<xs:simpleType>
																							<xs:restriction base="xs:decimal">
																								<xs:totalDigits value="15"/>
																								<xs:fractionDigits value="5"/>
																							</xs:restriction>
																						</xs:simpleType>
																					</xs:attribute>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																		<xs:attribute name="ПN" use="required">
																			<xs:annotation>
																				<xs:documentation>Номер по порядку</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:int">
																					<xs:minInclusive value="1"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="ИдПоставщика" type="xs:int" use="required">
																			<xs:annotation>
																				<xs:documentation>Идентификатор элемента справочника "Поставщики"</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Движение" minOccurs="0" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:attribute name="ПN" type="xs:int" use="required">
																			<xs:annotation>
																				<xs:documentation>Номер по порядку</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute name="П100000000006" use="required">
																			<xs:annotation>
																				<xs:documentation>Остаток на начало отчетного периода (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000007" use="required">
																			<xs:annotation>
																				<xs:documentation>Поступление (закупки) от организаций- производителей (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000008" use="required">
																			<xs:annotation>
																				<xs:documentation>Поступление (закупки) от организаций оптовой торговли (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000009" use="required">
																			<xs:annotation>
																				<xs:documentation>Поступление (закупки) по импорту (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000010" use="required">
																			<xs:annotation>
																				<xs:documentation>Поступление (закупки) итого (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000011" use="required">
																			<xs:annotation>
																				<xs:documentation>Поступление (возврат от покупателей) (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000012" use="required">
																			<xs:annotation>
																				<xs:documentation>Прочие поступления (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000013" use="required">
																			<xs:annotation>
																				<xs:documentation>Поступление всего (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000014" use="required">
																			<xs:annotation>
																				<xs:documentation>Расход (объем розничной продажи) (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000015" use="required">
																			<xs:annotation>
																				<xs:documentation>Прочий расход (дал)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:decimal">
																					<xs:totalDigits value="15"/>
																					<xs:fractionDigits value="5"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="П100000000016" use="required">
																			<xs:annotation>
																				<xs:documentation>Возврат поставщику (дал)</xs:documentation>
																			</xs:annotation>
