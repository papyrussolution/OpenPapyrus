					<xs:complexType>
						<xs:sequence>
							<xs:element name="E7008" minOccurs="0" maxOccurs="2">
								<xs:annotation>
									<xs:documentation>Описание товара</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="256"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MOA">
		<xs:annotation>
			<xs:documentation>Сумма</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="C516">
					<xs:annotation>
						<xs:documentation>Сумма</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="E5025">
								<xs:annotation>
									<xs:documentation>Квалификатор вида суммы
86 = Общая сумма заказа
128 = Сумма заказа за товарную позицию
203 = Сумма заказа за товарную позицию с учетом надбавок и скидок</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="3"/>
										<xs:enumeration value="86"/>
										<xs:enumeration value="128"/>
										<xs:enumeration value="203"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="E5004">
								<xs:annotation>
									<xs:documentation>Сумма</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="35"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
