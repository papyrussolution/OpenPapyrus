index:
	ID (unique);
	Country, Name acs (unique mod);
	Name acs (unique mod);
	Abbr acs (dup mod);
file:
	"region.btr";
	acstable = "rusncase.alt";
	balanced;
}
//
// Населенный пункт
// @obsolete (moved to table World)
//
table City {            // size=66
	autolong ID;        // Ид
	long   Country;     // Государство ->Country.ID
	long   Region;      // Регион      ->Region.ID
