	long   CashID;         // if (Flags & (CCHKF_SYNC|CCHKF_TRANSMIT)) then
		// (CashID ->Obj(PPOBJ_CASHNODE).ID) else (CashID - Code of cash machine)
	long   UserID;         // Пользователь, создавший чек
	long   SessID;         // ->CSession.ID
	date   Dt;             // Дата чека
	time   Tm;             // Время чека
	long   Flags;          // CCHKF_XXX
