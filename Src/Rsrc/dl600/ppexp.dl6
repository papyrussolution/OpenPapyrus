data QualityCert {
	key    ID;
	link   Person     PersonID;
	string Code[26];
	int16  Pad1;            // @alignment
	string Blank[16];
	string InnerCode[10];   // Внутренний код сертификата
	int16  Pad2;            // @alignment
	string GoodsName[48];   // Не зависит от поля Name таблицы Goods2
