<?xml version="1.0" encoding="utf-8"?>
<LinearLayout style="@style/ListItemRegular" xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/CTL_INCOMINGLIST_BILL_SCANMARKS_SETTING_LISTITEM"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    <TextView
        android:id="@+id/CTL_DOCUMENT_TI_GOODSNAME"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_marginLeft="2dp"
        android:textSize="12sp"
        android:textFontWeight="500"
        android:textColor="@color/TextPrimary"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <View style="@style/FakeView"/>
        <TextView
            android:id="@+id/CTL_DOCUMENT_TI_QTTY"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:minWidth="40dp"
            android:textSize="12sp"
            android:textFontWeight="600"
            android:layout_marginRight="8dp"
            android:textAlignment="viewEnd"
            android:textColor="@color/TextPrimary"
            />
    </LinearLayout>
    <ListView
        android:id="@+id/CTL_INCOMINGLIST_BILL_MARKSBYTI"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="2dp"
        android:layout_marginRight="2dp"
        android:divider="@android:color/transparent"
        android:dividerHeight="2dp"
        >
    </ListView>
</LinearLayout>